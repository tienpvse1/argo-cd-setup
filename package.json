{
	"name": "testing-container",
	"version": "0.0.1",
	"description": "",
	"author": "",
	"private": true,
	"license": "UNLICENSED",
	"scripts": {
		"build": "nest build",
		"start": "nest start",
		"start:dev": "nest start --watch",
		"start:debug": "nest start --debug --watch",
		"start:prod": "node dist/main",
		"code-quality:check": "pnpm exec biome check",
		"code-quality:fix": "pnpm exec biome check --fix",
		"test": "vitest run",
		"test:watch": "vitest",
		"test:cov": "vitest run --coverage",
		"test:debug": "vitest --inspect-brk --inspect --logHeapUsage --threads=false",
		"test:e2e": "vitest run --config ./vitest.config.e2e.ts",
		"migration:new": "touch ./migrations/$(date +%Y%m%d%H%M%S)_migration.ts",
		"migration:run": "kysely migrate:up",
		"migration:rollback": "kysely migrate:down"
	},
	"dependencies": {
		"@nestjs/common": "^11.0.1",
		"@nestjs/config": "^4.0.2",
		"@nestjs/core": "^11.0.1",
		"@nestjs/platform-express": "^11.0.1",
		"kysely": "^0.28.2",
		"pg": "^8.16.2",
		"reflect-metadata": "^0.2.2",
		"rxjs": "^7.8.1"
	},
	"devDependencies": {
		"@biomejs/biome": "2.0.5",
		"@golevelup/ts-vitest": "^0.5.2",
		"@nestjs/cli": "^11.0.0",
		"@nestjs/schematics": "^11.0.0",
		"@nestjs/testing": "^11.0.1",
		"@swc/cli": "^0.6.0",
		"@swc/core": "^1.12.5",
		"@types/express": "^5.0.0",
		"@types/jest": "^29.5.14",
		"@types/node": "^22.10.7",
		"@types/pg": "^8.15.4",
		"@types/supertest": "^6.0.2",
		"@vitest/coverage-v8": "^3.2.4",
		"dotenv": "^16.5.0",
		"globals": "^16.0.0",
		"kysely-ctl": "^0.13.1",
		"source-map-support": "^0.5.21",
		"supertest": "^7.0.0",
		"ts-loader": "^9.5.2",
		"ts-node": "^10.9.2",
		"tsconfig-paths": "^4.2.0",
		"typescript": "^5.7.3",
		"unplugin-swc": "^1.5.5",
		"vitest": "^3.2.4"
	},
	"jest": {
		"transform": {
			"^.+\\.(t|j)s?$": [
				"@swc/jest"
			]
		}
	},
	"packageManager": "pnpm@10.11.0+sha512.6540583f41cc5f628eb3d9773ecee802f4f9ef9923cc45b69890fb47991d4b092964694ec3a4f738a420c918a333062c8b925d312f42e4f0c263eb603551f977"
}
