{
	"compilerOptions": {
		"module": "commonjs",
		"declaration": true,
		"removeComments": true,
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		"allowSyntheticDefaultImports": true,
		"target": "ES2023",
		"sourceMap": true,
		"outDir": "./dist",
		"baseUrl": "./",
		"incremental": true,
		"skipLibCheck": true,
		"strictNullChecks": true,
		"forceConsistentCasingInFileNames": true,
		"noImplicitAny": false,
		"strictBindCallApply": false,
		"noFallthroughCasesInSwitch": false,
		"paths": {
			"@modules/*": [
				"./src/modules/*"
			],
			"@utils/*": [
				"./src/utils/*"
			],
			"@common/*": [
				"./src/common/*"
			],
			"@i18n/*": [
				"./src/i18n/*"
			],
			"@kysely": [
				"./src/kysely/index.ts"
			],
			"@kysely/*": [
				"./src/kysely/*"
			],
			"@auth": [
				"./src/auth/index.ts"
			],
			"@auth/*": [
				"./src/auth/*"
			]
		}
	}
}
